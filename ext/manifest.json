{
    "manifest_version": 2,
    "name": "Break The Rules",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://msn.khnu.km.ua/*", "http://msn.khnu.km.ua/*"],
            "js": ["levenshtein.js", "exam.js"]
        }
    ]
}